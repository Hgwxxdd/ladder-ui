  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "clean:lib": "rimraf lib",
    "clean:doc": "rimraf dist",
    "build:type": "node ./build/gen-types",
    "build:component": "rollup --config ./build/rollup.config.bundle.js",
    "build:css": "webpack --config ./build/webpack.config.css.js && rimraf lib/temp.css",
    "build:all": "yarn clean:lib && yarn build:component && yarn build:css",
    "test": "jest",
    "build:doc": "yarn clean:doc && cross-env NODE_ENV=production webpack --config ./build/webpack.doc.js",
    "build": "vue-cli-service build",
    "inspect": "vue-cli-service inspect > output.js"
  },
